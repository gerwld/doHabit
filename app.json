{
  "expo": {
    "name": "DoHabit",
    "slug": "doHabit-habits-tracker",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "dark": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
   }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.weblxapp.dohabithabitstracker.v1",
      "userInterfaceStyle": "automatic"
    },
    "android": {
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundImage": "./assets/adaptive-icon-background.png",
        "backgroundColor": "#3bb7e7",
        "monochromeImage": "./assets/adaptive-icon.png",
        "userInterfaceStyle": "automatic"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.weblxapp.dohabithabitstracker.v1"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "399bedba-025d-40d1-858c-e8203ef947ea"
      }
    }
  }
}
