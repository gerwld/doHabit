{
  "build": {
    "development": {
      "ios": {
        "cocoapods": "1.11.2",
        "cache": {
          "key": "my-project-cache-key",
          "paths": [
            "node_modules",
            "ios/Pods"
          ]
        }
      },
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "cache": {
          "key": "my-project-preview-cache-key",
          "paths": [
            "node_modules",
            "ios/Pods"
          ]
        }
      },
      "ios": {
        "simulator": true,
        "cache": {
          "key": "my-project-preview-cache-key",
          "paths": [
            "node_modules",
            "ios/Pods"
          ]
        }
      }
    },
    "production": {
      "distribution": "internal",
      "android": {
        "cache": {
          "key": "my-project-preview-cache-key",
          "paths": [
            "node_modules",
            "ios/Pods"
          ]
        }
      },
      "ios": {
        "simulator": true,
        "cache": {
          "key": "my-project-preview-cache-key",
          "paths": [
            "node_modules",
            "ios/Pods"
          ]
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}