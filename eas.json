{
  "build": {
    "development": {
      "ios": {
        "cocoapods": "1.11.2",
        "cache": {
          "key": "my-project-cache-key",
          "paths": ["node_modules", "ios/Pods"]
        }
      },
      "developmentClient": true,
      "distribution": "internal"
